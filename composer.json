{
    "name": "lairdubois/lairdubois",
    "license": "AGPLv3",
    "type": "project",
    "description": "L'Air du Bois",
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.2",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-imagick": "*",
        "ext-json": "*",

        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.2",
        "incenteev/composer-parameter-handler": "^2.0",
        "sensio/distribution-bundle": "^5.0.19",
        "sensio/framework-extra-bundle": "^5.0.0",
        "symfony/monolog-bundle": "^3.1.0",
        "symfony/polyfill-apcu": "^1.0",
        "symfony/swiftmailer-bundle": "^2.6.4",
        "symfony/symfony": "3.4.*",
        "symfony/intl": "^3.3",
        "twig/twig": "^2.0",
        "monolog/monolog": "^1.0",

        "doctrine/doctrine-migrations-bundle": "^1.3.1",
        "twig/extensions": "~1.0",
        "symfony/assetic-bundle": "dev-master",

        "friendsofsymfony/user-bundle": "*",
        "friendsofsymfony/elastica-bundle": "*",
        "stof/doctrine-extensions-bundle": "*",
        "liip/imagine-bundle": "~1.9",
        "gos/web-socket-bundle": "1.8.13",
        "bentools/webpush-bundle": "^0.3",
        "php-amqplib/rabbitmq-bundle": "^1.14",

        "facebook/graph-sdk": "~5.0",
        "willdurand/geocoder": "~3.0",
        "commerceguys/addressing": "^1.0",
        "twitter/bootstrap": "3.3.7",
        "uikit/uikit": "2.27.2",
        "afarkas/html5shiv": "3.7.3",
        "blueimp/jquery-file-upload": "9.28.0",
        "jublonet/codebird-php": "2.6.1",
        "emojione/emojione": "4.5.0",
        "eonasdan/bootstrap-datetimepicker": "4.17.47",
        "ps/image-optimizer": "1.2.0",
        "egulias/email-validator": "~1.2",
        "jmikola/geojson": "^1.0",
        "giggsey/libphonenumber-for-php": "^7.2",
        "cebe/markdown": "1.2.1",
        "stripe/stripe-php": "4.1.1",
        "hashids/hashids": "^1.0",
        "dirkgroenen/pinterest-api-php": "^0.2.11",
        "biblys/isbn": "2.0.6",
        "jaybizzle/crawler-detect" :"1.*",
        "suin/php-rss-writer": "^1.6",
        "chrome-php/chrome": "^0.7.0",

        "components/jquery": "3.3.1",
        "components/jqueryui": "1.12.1",
        "components/masonry": "3.2.2",
        "symfony/lock": "^3.4"

    },
    "require-dev": {
        "sensio/generator-bundle": "~3.0",
        "symfony/phpunit-bridge": "~3.0"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-root-package-install": [
            "SymfonyStandard\\Composer::hookRootPackageInstall"
        ],
        "post-install-cmd": [
            "@symfony-scripts",
            "Ladb\\CoreBundle\\Composer\\ScriptHandler::updateDirectoryStructure"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
